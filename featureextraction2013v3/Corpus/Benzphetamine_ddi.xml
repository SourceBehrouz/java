<?xml version="1.0" encoding="UTF-8" standalone="no"?><document id="DrugDDI.d218" origId="Benzphetamine"><sentence id="DrugDDI.d218.s0" origId="s0" text="Hypertensive crises have resulted when sympathomimetic amines have been used concomitantly within14 days following use of monoamine oxidase inhibitors."><entity charOffset="39-61" id="DrugDDI.d218.s0.e0" origId="s0.p4" text="sympathomimetic amines" type="drug"/><entity charOffset="122-150" id="DrugDDI.d218.s0.e1" origId="s0.p11" text="monoamine oxidase inhibitors" type="drug"/><pair e1="DrugDDI.d218.s0.e0" e2="DrugDDI.d218.s0.e1" id="DrugDDI.d218.s0.p0" interaction="true"/></sentence><sentence id="DrugDDI.d218.s1" origId="s1" text="DIDREX should not be used concomitantly with other CNS stimulants."><entity charOffset="0-6" id="DrugDDI.d218.s1.e0" origId="s1.p12" text="DIDREX" type="drug"/><entity charOffset="51-65" id="DrugDDI.d218.s1.e1" origId="s1.p18" text="CNS stimulants" type="drug"/><pair e1="DrugDDI.d218.s1.e0" e2="DrugDDI.d218.s1.e1" id="DrugDDI.d218.s1.p0" interaction="true"/><negationtags>DIDREX should &lt;xcope&gt;&lt;cue&gt;not&lt;/cue&gt; be used concomitantly with other CNS stimulants&lt;/xcope&gt;.</negationtags></sentence><sentence id="DrugDDI.d218.s2" origId="s2" text="Amphetamines may decrease the hypotensive effect of antihypertensives."><entity charOffset="0-12" id="DrugDDI.d218.s2.e0" origId="s2.p19" text="Amphetamines" type="drug"/><entity charOffset="52-69" id="DrugDDI.d218.s2.e1" origId="s2.p23" text="antihypertensives" type="drug"/><pair e1="DrugDDI.d218.s2.e0" e2="DrugDDI.d218.s2.e1" id="DrugDDI.d218.s2.p0" interaction="true"/></sentence><sentence id="DrugDDI.d218.s3" origId="s3" text="Amphetamines may enhance the effects of tricyclic antidepressants."><entity charOffset="0-12" id="DrugDDI.d218.s3.e0" origId="s3.p24" text="Amphetamines" type="drug"/><entity charOffset="40-65" id="DrugDDI.d218.s3.e1" origId="s3.p28" text="tricyclic antidepressants" type="drug"/><pair e1="DrugDDI.d218.s3.e0" e2="DrugDDI.d218.s3.e1" id="DrugDDI.d218.s3.p0" interaction="true"/></sentence><sentence id="DrugDDI.d218.s4" origId="s4" text="Urinary alkalinizing agents increase blood levels and decrease excretion of amphetamines."><entity charOffset="8-27" id="DrugDDI.d218.s4.e0" origId="s4.p29" text="alkalinizing agents" type="drug"/><entity charOffset="76-88" id="DrugDDI.d218.s4.e1" origId="s4.p34" text="amphetamines" type="drug"/><pair e1="DrugDDI.d218.s4.e0" e2="DrugDDI.d218.s4.e1" id="DrugDDI.d218.s4.p0" interaction="true"/></sentence><sentence id="DrugDDI.d218.s5" origId="s5" text="Urinary acidifying agents decrease blood levels and increase excretion of amphetamines."><entity charOffset="8-25" id="DrugDDI.d218.s5.e0" origId="s5.p35" text="acidifying agents" type="drug"/><entity charOffset="74-86" id="DrugDDI.d218.s5.e1" origId="s5.p40" text="amphetamines" type="drug"/><pair e1="DrugDDI.d218.s5.e0" e2="DrugDDI.d218.s5.e1" id="DrugDDI.d218.s5.p0" interaction="true"/></sentence></document>